{
  "name": "codecollab-server",
  "version": "2.0.0",
  "type": "module",
  "description": "Real-time collaborative coding platform with AI-powered hints and multi-language execution",
  "main": "app.js",
  "scripts": {
    "dev": "nodemon app.js",
    "start": "node app.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run --config vitest.integration.config.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:setup": "npm run db:generate && npm run db:migrate && npm run db:seed",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --name init",
    "db:push": "prisma db push",
    "db:seed": "node prisma/seed.js",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "docker:build": "docker build -t codecollab-server .",
    "docker:run": "docker run -p 3001:3001 codecollab-server"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@sentry/node": "^10.37.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.66.5",
    "chalk": "^5.3.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dockerode": "^4.0.2",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.9.2",
    "jsonwebtoken": "^9.0.3",
    "openai": "^4.20.0",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "piston-client": "^1.0.2",
    "prisma": "^5.22.0",
    "socket.io": "^4.6.1",
    "uuid": "^9.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.4",
    "vitest": "^4.0.17"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "collaborative-coding",
    "technical-interviews",
    "real-time",
    "socket.io",
    "code-execution",
    "ai-hints",
    "openai"
  ],
  "author": "MIMANGSHA",
  "license": "MIT"
}
